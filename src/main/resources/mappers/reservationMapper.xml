<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.dlqudwp.spring.test.ajax.repository.ReservationRepository">
 
 <select id="selectReservationList"
 resultType="com.dlqudwp.spring.test.ajax.domain.Reservation">
 
 	SELECT
 		`id`
 		, `name`
 		, `headcount`
 		, `day`
 		, `date`
 		, `phoneNumber`
 		, `state`
 		, `createdAt`
 		, `updatedAt`
 	FROM
 		`reservation`
 
 
 
 
 
 
 
 
 
 </select>
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 </mapper>